{
	"env": {
		"browser": true,
		"es6": true,
		"react-native/react-native": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": ["react", "react-native"]
}
